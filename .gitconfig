[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[push]
	default = current

[core]
	excludesfile = ~/.gitignore_global
[alias]
	o = checkout
	lol = log --graph --decorate --pretty=oneline --all --abbrev-commit
[merge]
	tool = vimdiff
